<%= myp_text_field(f, :short_description, "myplaceonline.recreational_vehicle_services.short_description", obj.short_description, autofocus) %>
<%= myp_date_field(f, :date_serviced, "myplaceonline.recreational_vehicle_services.date_serviced", obj.date_serviced) %>
<%= myp_text_field(f, :service_location, "myplaceonline.recreational_vehicle_services.service_location", obj.service_location, remote_autocomplete_model: obj.class, remote_autocomplete_all: true) %>
<%= myp_text_field(f, :cost, "myplaceonline.recreational_vehicle_services.cost", obj.cost) %>
<%= myp_text_area_markdown(f, :notes, "myplaceonline.recreational_vehicle_services.notes", obj.notes) %>
<%=
  render partial: "myplaceonline/pictures_form", locals: {
    f: f,
    obj: obj,
    position_field: :position,
    pictures_field: :recreational_vehicle_service_files,
    item_placeholder: "myplaceonline.recreational_vehicle_services.file",
    heading: "myplaceonline.recreational_vehicle_services.files",
    addbutton: "myplaceonline.recreational_vehicle_services.add_file",
    addbutton_multi: "myplaceonline.recreational_vehicle_services.add_files",
    deletebutton: "myplaceonline.recreational_vehicle_services.delete_file"
  }
%>
