<%= data_row(heading: t("myplaceonline.checks.description"), content: obj.description) %>
<%= data_row(heading: t("myplaceonline.checks.amount"), content: obj.amount) %>
<%= data_row(heading: t("myplaceonline.checks.received_date"), content: obj.received_date) %>
<%= data_row(heading: t("myplaceonline.checks.deposit_date"), content: obj.deposit_date) %>
<%= attribute_table_row_reference(t("myplaceonline.checks.contact"), "contact_path", obj.contact) %>
<%= attribute_table_row_reference(t("myplaceonline.checks.company"), "company_path", obj.company) %>
<%= attribute_table_row_reference(t("myplaceonline.checks.bank_account"), "bank_account_path", obj.bank_account) %>
<%= data_row(heading: t("myplaceonline.general.notes"), content: obj.notes, markdown: true) %>
<%=
  render partial: "myplaceonline/pictures", locals: {
    pics: obj.check_files,
    placeholder: "myplaceonline.identity_files.file"
  }
%>
