<%=
  render partial: 'myplaceonline/select_or_create', locals: {
    f: f,
    name: :contact,
    existingobj: obj.contact,
    heading: t("myplaceonline.permissions.contact"),
    objform: "contacts/form",
    existingobjurl: "contact_path",
    searchurl: "/contacts.json?perpage=0",
    edit: edit,
    autofocus: autofocus
  }
%>
<%= myp_select(f, :action, "myplaceonline.permissions.action", Myp.translate_options(Permission::ACTION_TYPES), obj.action) %>
<%= myp_select(f, :subject_class, "myplaceonline.permissions.subject_class", Myp.get_category_list, obj.subject_class) %>
<%= myp_number_field(f, :subject_id, "myplaceonline.permissions.subject_id", obj.subject_id) %>
