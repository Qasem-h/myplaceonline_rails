<%= render layout: "shared/model_index" do %>
  <% content_for :abovelist do %>
    <% if !@message.blank? %>
      <div class="errors">
        <ul><li><%= @message %></li></ul>
      </div>
    <% end %>
  <% end %>
  <%=
    render partial: 'myplaceonline/sort_items', locals: {
      options: controller.index_sorts
    }
  %>
  <% content_for :footerlinks do %>
    <li><%= link_to t('myplaceonline.feeds.load_all'), feeds_load_all_path, "data-icon" => "refresh" %></li>
  <% end %>
<% end %>
