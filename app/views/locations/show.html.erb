<%= render layout: "shared/model_show" do %>

  <%= attribute_table_row(t("myplaceonline.locations.name"), @obj.name) %>
  <%= attribute_table_row(t("myplaceonline.locations.address1"), @obj.address1) %>
  <%= attribute_table_row(t("myplaceonline.locations.address2"), @obj.address2) %>
  <%= attribute_table_row(t("myplaceonline.locations.address3"), @obj.address3) %>
  <%= attribute_table_row(t("myplaceonline.locations.sub_region2"), @obj.sub_region2) %>
  <%= attribute_table_row(t("myplaceonline.locations.sub_region1"), @obj.sub_region1_name) %>
  <%= attribute_table_row(t("myplaceonline.locations.postal_code"), @obj.postal_code) %>
  <%= attribute_table_row(t("myplaceonline.locations.region"), @obj.region_name) %>
  <%= attribute_table_row_url(t("myplaceonline.locations.map"), @obj.map_url) %>
  <% @obj.location_phones.each do |phone| %>
  
    <%= attribute_table_row(t("myplaceonline.locations.phone"), phone.number) %>

  <% end %>

<% end %>
