<%=
  render partial: 'myplaceonline/select_or_create', locals: {
    f: f,
    name: :location,
    existingobj: obj.location,
    heading: t("myplaceonline.hotels.location"),
    objform: "locations/form",
    existingobjurl: "location_path",
    searchurl: "/locations.json?perpage=0",
    edit: edit,
    autofocus: true
  }
%>
<%= myp_select(f, :overall_rating, "myplaceonline.hotels.overall_rating", Myp.translate_options(Myp::RATINGS), obj.overall_rating) %>
<%= myp_select(f, :breakfast_rating, "myplaceonline.hotels.breakfast_rating", Myp.translate_options(Myp::RATINGS), obj.breakfast_rating) %>
<%= myp_text_area_markdown(f, :notes, "myplaceonline.general.notes", obj.notes) %>
