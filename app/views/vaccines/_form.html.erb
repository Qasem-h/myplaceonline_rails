<%= myp_text_field(f, :vaccine_name, "myplaceonline.vaccines.vaccine_name", obj.vaccine_name, autofocus) %>
<%= myp_date_field(f, :vaccine_date, "myplaceonline.vaccines.vaccine_date", obj.vaccine_date) %>
<%= myp_text_area_markdown(f, :notes, "myplaceonline.general.notes", obj.notes, collapsed: false) %>
<%=
  render partial: "myplaceonline/pictures_form", locals: {
    f: f,
    obj: obj,
    position_field: :position,
    pictures_field: :vaccine_files,
    item_placeholder: "myplaceonline.vaccines.file",
    heading: "myplaceonline.vaccines.files",
    addbutton: "myplaceonline.vaccines.add_file",
    addbutton_multi: "myplaceonline.vaccines.add_files",
    deletebutton: "myplaceonline.vaccines.delete_file"
  }
%>
