<%= render layout: "shared/model_show" do %>

  <%= attribute_table_row(t("myplaceonline.feed_items.feed_title"), @obj.feed_title) %>
  <%= attribute_table_row_url(t("myplaceonline.feed_items.feed_link"), @obj.feed_link) %>
  <%= attribute_table_row_datetime(t("myplaceonline.feed_items.publication_date"), @obj.publication_date) %>
  <%= attribute_table_row(t("myplaceonline.feed_items.guid"), @obj.guid) %>
  <%= attribute_table_row(t("myplaceonline.feed_items.content"), @obj.content.html_safe) %>

  <% content_for :postfooterlinks do %>
    <li><%= link_to t('myplaceonline.feed_items.feed'), feed_path(@obj.feed), "data-icon" => "back" %></li>
  <% end %>
<% end %>
