<div data-role="collapsible" data-collapsed="true">
  <h3><%= heading %></h3>
  <div data-role="collapsible-set">
    <div data-role="collapsible" data-collapsed="false">
      <h3><%= t("myplaceonline.calculation_forms.constant") %></h3>
      <%= myp_text_field f, :constant_value, "myplaceonline.calculation_forms.constant_value", val %>
    </div>
    <div data-role="collapsible" data-collapsed="true">
      <h3><%= t("myplaceonline.calculation_forms.sub_element") %></h3>
      <div class="itemswrapper">
        <p><a href="#" onclick="return form_add_item(this, '<%= f.object_name + "[test_attributes]" %>', 'Delete', [{ type: 'calculation_element', name: 'name' }], '<%= t("myplaceonline.general.singleton_form_item") %>');" class="ui-btn"><%= t("myplaceonline.calculation_forms.create_sub_element") %></a></p>
      </div>
    </div>
  </div>
</div>
