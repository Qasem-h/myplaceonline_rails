<%= data_row(heading: t("myplaceonline.food_informations.food_name"), content: obj.food_name) %>
<%= data_row(heading: t("myplaceonline.food_informations.weight"), content: Myp.decimal_to_s(value: obj.usda_weight.gram_weight)) %>
<%= data_row(heading: t("myplaceonline.food_informations.calories"), content: Myp.decimal_to_s(value: obj.calories)) %>
<% @nutrients.each do |food_nutrient| %>
  <%= data_row(heading: food_nutrient.nutrient.nutrient_description, content: "#{obj.nutrient_value(food_nutrient: food_nutrient)}  #{food_nutrient.nutrient.units}") %>
<% end %>
<%= data_row(heading: t("myplaceonline.general.notes"), content: obj.notes, markdown: true) %>
