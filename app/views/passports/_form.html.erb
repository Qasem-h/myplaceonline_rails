<%= myp_region_field(f, :region, "myplaceonline.passports.region", obj.region, autofocus) %>
<%= myp_text_field(f, :name, "myplaceonline.passports.name", obj.name) %>
<%= myp_text_field(f, :passport_number, "myplaceonline.passports.passport_number", obj.passport_number) %>
<%= myp_date_field(f, :expires, "myplaceonline.passports.expires", obj.expires) %>
<%= myp_date_field(f, :issued, "myplaceonline.passports.issued", obj.issued) %>
<%= myp_text_field(f, :issuing_authority, "myplaceonline.passports.issuing_authority", obj.issuing_authority) %>
<%= myp_text_area_markdown(f, :notes, "myplaceonline.general.notes", obj.notes) %>
<%=
  render partial: "myplaceonline/pictures_form", locals: {
    f: f,
    obj: obj,
    pictures_field: :passport_pictures
  }
%>
