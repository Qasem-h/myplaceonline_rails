<%= render layout: "shared/model_show" do %>

  <%= attribute_table_row(t("myplaceonline.passports.region"), @obj.region) %>
  <%= attribute_table_row(t("myplaceonline.passports.name"), @obj.name) %>
  <%= attribute_table_row(t("myplaceonline.passports.passport_number"), @obj.passport_number) %>
  <%= attribute_table_row_date(t("myplaceonline.passports.expires"), @obj.expires) %>
  <%= attribute_table_row_date(t("myplaceonline.passports.issued"), @obj.issued) %>
  <%= attribute_table_row(t("myplaceonline.passports.issuing_authority"), @obj.issuing_authority) %>
  <% @obj.passport_pictures.each do |pic| %>
    <%= table_row_heading(t("myplaceonline.passports.picture")) %>
    <%= attribute_table_row_image(t("myplaceonline.passports.picture"), pic.identity_file) %>
  <% end %>

<% end %>
