<%= attribute_table_row(t("myplaceonline.project_issues.issue_name"), obj.issue_name) %>
<%= attribute_table_row_markdown(t("myplaceonline.general.notes"), obj.notes) %>
<%= attribute_table_row_boolean(t("myplaceonline.general.archived"), !obj.archived.nil?) %>

<% obj.project_issue_notifiers.each do |wrapper| %>
  <%= attribute_table_row_reference(t("myplaceonline.project_issues.notifier"), "contact_path", wrapper.contact) %>
<% end %>

<%=
  render partial: 'myplaceonline/pictures', locals: {
    pics: @obj.project_issue_files,
    placeholder: "myplaceonline.project_issues.file"
  }
%>
