<% if !@myplet %>
<%= attribute_table_row(t("myplaceonline.notepads.title"), obj.title) %>
<%= attribute_table_row_markdown(t("myplaceonline.notepads.notepad_data"), obj.notepad_data) %>
<%= attribute_table_row_boolean(t("myplaceonline.general.archived"), !obj.archived.nil?) %>
<% else %>
<%= form_for obj, :url => @url, :remote => controller.may_upload, data: { ajax: !controller.may_upload } do |f| %>
  <%= render partial: "shared/model_form", locals: { form_name: controller.paths_name + "/form", edit: true, autofocus: true, f: f } %>
<% end %>
<% end %>
