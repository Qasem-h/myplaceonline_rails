<%= myp_text_field(f, :title, "myplaceonline.myplets.title", Myp.evaluate_if_probably_i18n(obj.title)) %>
  <%# myp_text_field(f, :category_name, "myplaceonline.myplets.category_name", obj.category_name) %>
  <%# myp_number_field(f, :category_id, "myplaceonline.myplets.category_id", obj.category_id) %>
  <%# myp_select(f, :border_type, "myplaceonline.myplets.border_type", Myp.translate_options(Myplet::BORDER_TYPES), obj.border_type) %>

<% if !@category_obj.nil? %>
  <%= f.fields_for @obj.category_name.singularize, @category_obj do |sub_fields| %>
    <%= render partial: @obj.category_name + "/form", locals: { obj: @category_obj, edit: edit, autofocus: false, f: sub_fields, myplet_form: true } %>
  <% end %>
<% end %>
