  <%= myp_date_field(child_fields, :end_date, "myplaceonline.job_managers.end_date", childproperty.end_date) %>
  <%= myp_text_area_markdown(child_fields, :notes, "myplaceonline.general.notes", childproperty.notes) %>
<% end %>
