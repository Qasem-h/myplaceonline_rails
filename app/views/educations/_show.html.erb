<%= attribute_table_row(t("myplaceonline.educations.education_name"), obj.education_name) %>
<%= attribute_table_row_date(t("myplaceonline.educations.education_start"), obj.education_start) %>
<%= attribute_table_row_date(t("myplaceonline.educations.education_end"), obj.education_end) %>
<%= attribute_table_row_select(t("myplaceonline.educations.degree_type"), obj.degree_type, Education::EDUCATION_TYPES) %>
<%= attribute_table_row(t("myplaceonline.educations.degree_name"), obj.degree_name) %>
<%= attribute_table_row(t("myplaceonline.educations.gpa"), obj.gpa) %>
<%= attribute_table_row_markdown(t("myplaceonline.general.notes"), obj.notes) %>
<%= attribute_table_row_reference(t("myplaceonline.educations.location"), "location_path", obj.location) %>
<%=
  render partial: 'myplaceonline/pictures', locals: {
    pics: @obj.education_files,
    placeholder: "myplaceonline.educations.file"
  }
%>
