<%= data_row(heading: t("myplaceonline.prescriptions.prescription_name"), content: obj.prescription_name) %>
<%= data_row(heading: t("myplaceonline.prescriptions.prescription_date"), content: obj.prescription_date) %>
<%= data_row(heading: t("myplaceonline.prescriptions.refill_maximum"), content: obj.refill_maximum) %>
<%= data_row(heading: t("myplaceonline.general.notes"), content: obj.notes, markdown: true) %>
<%= attribute_table_row_reference(t("myplaceonline.prescriptions.doctor"), "doctor_path", obj.doctor) %>
<% obj.prescription_refills.each do |x| %>
  <%= table_row_heading(t("myplaceonline.prescriptions.refill")) %>
  <%= render partial: "prescription_refills/show", locals: { obj: x } %>
<% end %>
<%=
  render partial: "myplaceonline/pictures", locals: {
    pics: obj.prescription_files,
    placeholder: "myplaceonline.identity_files.file"
  }
%>
