<%= myp_text_field(f, :playlist_name, "myplaceonline.playlists.playlist_name", obj.playlist_name, autofocus) %>
<%=
  render layout: 'myplaceonline/childproperties', locals: {
    f: f,
    heading: t("myplaceonline.playlists.songs"),
    childpropertiesname: :playlist_songs,
    childproperties: obj.playlist_songs,
    deletebutton: t("myplaceonline.playlists.delete_song"),
    addbutton: t("myplaceonline.playlists.add_song"),
    expanded: true,
    position_field: :position,
    itemname: t("myplaceonline.playlists.song"),
    formjson: [
      {
        type: :renderpartial,
        name: :song_attributes,
        partial: 'myplaceonline/create',
        objform: "songs/form",
        autofocus: false
      },
      {
        type: 'position',
        name: 'position'
      }
    ]
  } do |child_fields, childproperty|
%>
  <%=
    render partial: 'myplaceonline/create', locals: {
      f: child_fields,
      name: :song_attributes,
      existingobj: childproperty.song,
      objform: "songs/form",
      autofocus: false
    }
  %>
<% end %>
