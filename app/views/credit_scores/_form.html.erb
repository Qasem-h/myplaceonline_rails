<%= myp_date_field(f, :score_date, "myplaceonline.credit_scores.score_date", obj.score_date) %>
<%= myp_number_field(f, :score, "myplaceonline.credit_scores.score", obj.score) %>
<%= myp_text_field(f, :source, "myplaceonline.credit_scores.source", obj.source, remote_autocomplete_model: obj.class, remote_autocomplete_all: true) %>
<%=
  render partial: "myplaceonline/pictures_form", locals: {
    f: f,
    obj: obj,
    position_field: :position,
    pictures_field: :credit_score_files,
    item_placeholder: "myplaceonline.identity_files.file",
    heading: "myplaceonline.identity_files.files",
    addbutton: "myplaceonline.identity_files.add_file",
    addbutton_multi: "myplaceonline.identity_files.add_files",
    deletebutton: "myplaceonline.identity_files.delete_file"
  }
%>
