<%= myp_text_field(f, :gun_name, "myplaceonline.guns.gun_name", obj.gun_name, autofocus) %>
<%= myp_text_field(f, :gun_model, "myplaceonline.guns.gun_model", obj.gun_model) %>
<%= myp_text_field(f, :manufacturer_name, "myplaceonline.guns.manufacturer_name", obj.manufacturer_name) %>
<%= myp_decimal_field(f, :bullet_caliber, "myplaceonline.guns.bullet_caliber", obj.bullet_caliber) %>
<%= myp_integer_field(f, :max_bullets, "myplaceonline.guns.max_bullets", obj.max_bullets) %>
<%= myp_decimal_field(f, :price, "myplaceonline.guns.price", obj.price) %>
<%= myp_date_field(f, :purchased, "myplaceonline.guns.purchased", obj.purchased) %>
<%= myp_text_area_markdown(f, :notes, "myplaceonline.guns.notes", obj.notes) %>
<%=
  render(layout: 'myplaceonline/childproperties', locals: {
    f: f,
    heading: t("myplaceonline.gun_registrations.gun_registrations"),
    childpropertiesname: :gun_registrations,
    childproperties: obj.gun_registrations,
    deletebutton: t("myplaceonline.gun_registrations.delete"),
    addbutton: t("myplaceonline.gun_registrations.add"),
    expanded: obj.gun_registrations.length > 0,
    formjson: [
      {
        type: :renderpartial,
        name: :location_attributes,
        partial: 'myplaceonline/select_or_create',
        heading: t("myplaceonline.gun_registrations.location"),
        objform: "locations/form",
        existingobjurl: "location_path",
        searchurl: "/locations.json?perpage=0",
        edit: true,
        autofocus: true
      },
      {
        type: 'date',
        name: 'registered',
        placeholder: t("myplaceonline.gun_registrations.registered")
      },
      {
        type: 'date',
        name: 'expires',
        placeholder: t("myplaceonline.gun_registrations.expires")
      }
    ]
  }) do |child_fields, childproperty|
%>
  <%=
    render partial: 'myplaceonline/select_or_create', locals: {
      f: child_fields,
      name: :location_attributes,
      existingobj: childproperty.location,
      heading: t("myplaceonline.gun_registrations.location"),
      objform: "locations/form",
      existingobjurl: "location_path",
      searchurl: "/locations.json?perpage=0",
      edit: true,
      autofocus: false
    }
  %>
  <%= myp_date_field(child_fields, :registered, "myplaceonline.gun_registrations.registered", childproperty.registered) %>
  <%= myp_date_field(child_fields, :expires, "myplaceonline.gun_registrations.expires", childproperty.expires) %>
<% end %>
