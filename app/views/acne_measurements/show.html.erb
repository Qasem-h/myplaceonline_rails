<%= render layout: "shared/model_show" do %>

  <%= attribute_table_row_datetime(t("myplaceonline.acne_measurements.measurement_datetime"), @obj.measurement_datetime) %>
  <%= attribute_table_row(t("myplaceonline.acne_measurements.new_pimples"), @obj.new_pimples) %>
  <%= attribute_table_row(t("myplaceonline.acne_measurements.worrying_pimples"), @obj.worrying_pimples) %>
  <%= attribute_table_row(t("myplaceonline.acne_measurements.total_pimples"), @obj.total_pimples) %>
  <%= attribute_table_row(t("myplaceonline.acne_measurements.acne_location"), @obj.acne_location) %>
  <% @obj.acne_measurement_pictures.each do |pic| %>
    <%= table_row_heading(t("myplaceonline.acne_measurements.picture")) %>
    <% if !pic.identity_file.nil? && !pic.identity_file.file_content_type.nil? && (pic.identity_file.file_content_type.start_with?("image")) %>
      <%= attribute_table_row_image(t("myplaceonline.acne_measurements.picture"), pic.identity_file) %>
    <% end %>
  <% end %>
    
<% end %>
