<%= render layout: "shared/model_show" do %>

  <%= attribute_table_row(t("myplaceonline.websites.title"), @obj.title) %>
  <%= attribute_table_row_url(t("myplaceonline.websites.url"), @obj.url) %>
  <%= attribute_table_row(t("myplaceonline.websites.website_category"), @obj.website_category) %>
  <%= attribute_table_row_boolean(t("myplaceonline.websites.to_visit"), @obj.to_visit) %>
  <% @obj.website_passwords.each do |wrapper| %>
    <%= attribute_table_row_reference(t("myplaceonline.websites.password"), "password_path", wrapper.password) %>
  <% end %>
  <%= attribute_table_row_reference(t("myplaceonline.websites.recommender"), "contact_path", @obj.recommender) %>
  <%= attribute_table_row_markdown(t("myplaceonline.general.notes"), @obj.notes) %>
  
<% end %>
