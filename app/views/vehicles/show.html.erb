<%= render layout: "shared/model_show" do %>

  <%= attribute_table_row(t("myplaceonline.vehicles.name"), @obj.name) %>
  <%= attribute_table_row(t("myplaceonline.vehicles.vin"), @obj.vin) %>
  <%= attribute_table_row(t("myplaceonline.vehicles.manufacturer"), @obj.manufacturer) %>
  <%= attribute_table_row(t("myplaceonline.vehicles.model"), @obj.model) %>
  <%= attribute_table_row(t("myplaceonline.vehicles.year"), @obj.year) %>
  <%= attribute_table_row(t("myplaceonline.vehicles.color"), @obj.color) %>
  <%= attribute_table_row(t("myplaceonline.vehicles.owned_start"), Myp.display_date(@obj.owned_start, current_user)) %>
  <%= attribute_table_row(t("myplaceonline.vehicles.owned_end"), Myp.display_date(@obj.owned_end, current_user)) %>
  <%= attribute_table_row(t("myplaceonline.vehicles.license_plate"), @obj.license_plate) %>
  <%= attribute_table_row(t("myplaceonline.general.region"), @obj.region) %>
  <%= attribute_table_row(t("myplaceonline.general.sub_region1"), @obj.sub_region1) %>
  <%= attribute_table_row_markdown(t("myplaceonline.vehicles.notes"), @obj.notes) %>
  <% @obj.vehicle_loans.each do |loan| %>
    <%= attribute_table_row(t("myplaceonline.vehicle_loans.lender"), loan.lender) %>
    <%= attribute_table_row(t("myplaceonline.vehicle_loans.amount"), loan.amount) %>
    <%= attribute_table_row(t("myplaceonline.vehicle_loans.monthly_payment"), loan.monthly_payment) %>
    <%= attribute_table_row(t("myplaceonline.vehicle_loans.start"), Myp.display_date(loan.start, current_user)) %>
    <%= attribute_table_row(t("myplaceonline.vehicle_loans.paid_off"), Myp.display_date(loan.paid_off, current_user)) %>
  <% end %>
  <% @obj.vehicle_services.each do |service| %>
    <%= attribute_table_row(t("myplaceonline.vehicle_service.short_description"), service.short_description) %>
    <%= attribute_table_row(t("myplaceonline.vehicle_service.date_due"), Myp.display_date(service.date_due, current_user)) %>
    <%= attribute_table_row(t("myplaceonline.vehicle_service.date_serviced"), Myp.display_date(service.date_serviced, current_user)) %>
    <%= attribute_table_row(t("myplaceonline.vehicle_service.service_location"), service.service_location) %>
    <%= attribute_table_row(t("myplaceonline.vehicle_service.cost"), service.cost) %>
    <%= attribute_table_row(t("myplaceonline.vehicle_service.miles"), service.miles) %>
    <%= attribute_table_row_markdown(t("myplaceonline.vehicle_service.notes"), service.notes) %>
  <% end %>

<% end %>