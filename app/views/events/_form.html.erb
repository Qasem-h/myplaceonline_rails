<%= myp_text_field(f, :event_name, "myplaceonline.events.event_name", obj.event_name, autofocus) %>
<%= myp_datetime_field(f, :event_time, "myplaceonline.events.event_time", obj.event_time) %>
<%= myp_datetime_field(f, :event_end_time, "myplaceonline.events.event_end_time", obj.event_end_time) %>
<%= myp_text_area_markdown(f, :notes, "myplaceonline.events.notes", obj.notes) %>
<%=
  render partial: 'myplaceonline/create', locals: {
    f: f,
    name: :repeat_attributes,
    heading: Myp.heading("repeats"),
    existingobj: obj.repeat,
    objform: "repeats/form",
    autofocus: false
  }
%>
<%=
  render partial: 'myplaceonline/select_or_create', locals: {
    f: f,
    name: :location,
    existingobj: obj.location,
    heading: t("myplaceonline.events.location"),
    objform: "locations/form",
    existingobjurl: "location_path",
    searchurl: "/locations.json?perpage=0",
    edit: edit,
    autofocus: false
  }
%>
<%=
  render layout: 'myplaceonline/childproperties', locals: {
    f: f,
    heading: t("myplaceonline.events.pictures"),
    childpropertiesname: :event_pictures,
    childproperties: obj.event_pictures,
    deletebutton: t("myplaceonline.events.delete_picture"),
    addbutton: t("myplaceonline.events.add_picture"),
    expanded: obj.event_pictures.length > 0,
    formjson: [
      {
        type: 'file',
        name: 'identity_file_attributes.file',
        placeholder: t("myplaceonline.events.picture")
      }
    ]
  } do |child_fields, childproperty|
%>
  <%= child_fields.fields_for :identity_file, childproperty.identity_file do |file_fields| %>
    <%= myp_file_field file_fields, :file, "myplaceonline.events.picture", childproperty.identity_file.file %>
  <% end %>
<% end %>
