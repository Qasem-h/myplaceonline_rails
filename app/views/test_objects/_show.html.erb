<%= attribute_table_row(t("myplaceonline.test_objects.test_object_name"), obj.test_object_name) %>
<%= attribute_table_row_markdown(t("myplaceonline.general.notes"), obj.notes) %>
<% obj.test_object_instances.each do |x| %>
  <%= table_row_heading(t("myplaceonline.test_objects.instance")) %>
  <%= render partial: "test_object_instances/show", locals: { obj: x } %>
<% end %>
<%=
  render partial: "myplaceonline/pictures", locals: {
    pics: obj.test_object_files,
    placeholder: "myplaceonline.identity_files.file"
  }
%>
