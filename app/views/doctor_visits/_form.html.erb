<%= myp_date_field(f, :visit_date, "myplaceonline.doctor_visits.visit_date", obj.visit_date) %>
<%= myp_decimal_field(f, :paid, "myplaceonline.doctor_visits.paid", obj.paid) %>
<%= myp_check_box(f, :physical, "myplaceonline.doctor_visits.physical") %>
<%= myp_text_area_markdown(f, :notes, "myplaceonline.doctor_visits.notes", obj.notes) %>
<%=
  render partial: 'myplaceonline/select_or_create', locals: {
    f: f,
    name: :doctor,
    existingobj: obj.doctor,
    heading: t("myplaceonline.doctor_visits.doctor"),
    objform: "doctors/form",
    existingobjurl: "doctor_path",
    searchurl: "/doctors.json?perpage=0",
    edit: edit,
    autofocus: false
  }
%>
<%=
  render partial: 'myplaceonline/select_or_create', locals: {
    f: f,
    name: :health_insurance,
    existingobj: obj.health_insurance,
    heading: t("myplaceonline.doctor_visits.health_insurance"),
    objform: "health_insurances/form",
    existingobjurl: "health_insurance_path",
    searchurl: "/health_insurances.json?perpage=0",
    edit: edit,
    autofocus: false,
    collapsed: obj.health_insurance.nil?
  }
%>
