<%= myp_date_field(f, :conversation_date, "myplaceonline.contacts.conversation_date", obj.conversation_date) %>
<%=
  render partial: "myplaceonline/pictures_form", locals: {
    f: f,
    obj: obj,
    position_field: :position,
