<%= render layout: "shared/model_show" do %>

  <%= attribute_table_row_datetime(t("myplaceonline.meals.meal_time"), @obj.meal_time) %>
  <% @obj.meal_foods.each do |wrapper| %>
    <%= table_row_heading(t("myplaceonline.foods.heading")) %>
    <%= attribute_table_row(t("myplaceonline.foods.food_name"), wrapper.food.food_name) %>
    <%= attribute_table_row(t("myplaceonline.foods.price"), wrapper.food.price) %>
    <%= attribute_table_row(t("myplaceonline.foods.calories"), wrapper.food.calories) %>
    <%= attribute_table_row_markdown(t("myplaceonline.foods.notes"), wrapper.food.notes) %>
  <% end %>
  <%= attribute_table_row(t("myplaceonline.meals.price"), @obj.price) %>
  <%= attribute_table_row(t("myplaceonline.meals.calories"), @obj.calories) %>
  <%= attribute_table_row_markdown(t("myplaceonline.meals.notes"), @obj.notes) %>
  <%= attribute_table_row_reference(t("myplaceonline.meals.location"), "location_path", @obj.location) %>
  
<% end %>
