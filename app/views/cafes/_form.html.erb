<%=
  render partial: "myplaceonline/select_or_create", locals: {
    f: f,
    name: :location,
    existingobj: obj.location,
    heading: t("myplaceonline.cafes.location"),
    objform: "locations/form",
    existingobjurl: "location_path",
    searchurl: "/locations.json?perpage=0",
    edit: edit,
    autofocus: true,
    expanded_if_nil: true
  }
%>
<%= myp_select(f, :rating, "myplaceonline.cafes.rating", Myp.translate_options(Myp::RATINGS), obj.rating) %>
<%= myp_text_area_markdown(f, :notes, "myplaceonline.cafes.notes", obj.notes) %>
